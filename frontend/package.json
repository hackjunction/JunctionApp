        "apollo-boost": "^0.4.7",
        "apollo-link-context": "^1.0.19",
        "apollo-link-error": "^1.1.12",
        "auth0-js": "^9.12.2",
        "axios": "^0.18.0",
        "babel-plugin-import": "^1.12.2",
        "cloudinary-react": "^1.1.1",
        "connected-react-router": "^6.3.0",
        "formik": "^2.0.8",
        "framer-motion": "^1.4.2",
        "graphql": "^14.5.8",
        "lodash-es": "^4.17.11",
        "logrocket": "^1.0.5",
        "moment": "^2.24.0",
        "moment-timezone": "^0.5.25",
        "notistack": "^0.9.2",
        "object-path": "^0.11.4",
        "qrcode.react": "^0.9.3",
